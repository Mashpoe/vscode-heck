{
    "name": "heck",
    "displayName": "heck",
    "description": "Syntax highlighting for the Heck programming language.",
    "version": "0.0.4",
    "publisher": "Mashpoe",
    "icon": "images/icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/Mashpoe/vscode-heck.git"
    },
    "engines": {
        "vscode": "^1.39.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "heck",
            "aliases": ["Heck", "heck"],
            "extensions": [".heck"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "heck",
            "scopeName": "source.heck",
            "path": "./syntaxes/heck.tmLanguage.json"
        }]
    }
}